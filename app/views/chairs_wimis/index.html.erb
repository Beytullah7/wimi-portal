<h1>Listing Chairs Wimis</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Chair</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chairs_wimis.each do |chairs_wimi| %>
      <tr>
        <td><%= chairs_wimi.user %></td>
        <td><%= chairs_wimi.chair %></td>
