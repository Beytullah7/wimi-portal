<%- model_class = TravelExpenseReportItem -%>
<li>
  <%= model_class.human_attribute_name(:date) %>: <%= l travel_expense_report_item.date %>
  <%= model_class.human_attribute_name(:breakfast) %>: <%= t(travel_expense_report_item.breakfast.to_s) %>
  <%= model_class.human_attribute_name(:lunch) %>: <%= t(travel_expense_report_item.lunch.to_s) %>
  <%= model_class.human_attribute_name(:dinner) %>: <%= t(travel_expense_report_item.dinner.to_s) %>
  <%= model_class.human_attribute_name(:annotation) %>: <%= travel_expense_report_item.annotation %>
</li>
